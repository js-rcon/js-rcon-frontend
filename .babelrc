{
  "presets": [
    "react",
    [ "env", { "modules": false } ]
  ],
  "plugins": [
    [ "emotion", { "hoist": true } ],
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "transform-class-properties",
    [ "transform-runtime", { "polyfill": false, "regenerator": true } ]
  ]
}
